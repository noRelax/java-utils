<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="com.message.bean">
<class name="ErShouMai" table="ershoumai">
	<id name="id" column="id" type="integer">
		<generator class="native"></generator>
	</id>
	<property name="admin" column="admin" type="integer"></property>
	<property name="title" column="title" type="string"></property>
	<property name="content" column="content" type="string"></property>
	<property name="time" column="time" type="string" insert="false"></property>
</class>
</hibernate-mapping>
